RULE: STX_EXPR
-
	AND
		GROUP
			AND
				IFONE
					SYNTAX --- STX_NOT
				OR
					SYNTAX --- STX_GEXPR
					SYNTAX --- STX_FNCALL
					SYNTAX --- STX_VAR
					SYNTAX --- STX_MEMBER
					SYNTAX --- STX_INDEX
					SYNTAX --- STX_NUM
					SYNTAX --- STX_STRING
					SYNTAX --- STX_TRUE
					SYNTAX --- STX_FALSE
		IFONE
			AND
				OR
					SYNTAX --- STX_ARITHOP
					SYNTAX --- STX_BOOLOP
				OR
					SYNTAX --- STX_GEXPR
					SYNTAX --- STX_EXPR

