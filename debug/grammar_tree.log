RULE: STX_SCOPE
-
	AND
		IFONE
			AND
				TOKEN --- TK_SCOPE
				TOKEN --- TK_IDENTIFIER
		TOKEN --- TK_LBRACE
		OR
			SYNTAX --- STX_CLASS
			SYNTAX --- STX_FNDEF
			SYNTAX --- STX_IF
			SYNTAX --- STX_WHILE
			SYNTAX --- STX_FOR
			SYNTAX --- STX_DECLARE
			SYNTAX --- STX_ASSIGN
			SYNTAX --- STX_INIT
			SYNTAX --- STX_ECHO
			SYNTAX --- STX_BREAK
			SYNTAX --- STX_RETURN
			SYNTAX --- STX_EXIT
		IFMANY
			OR
				SYNTAX --- STX_CLASS
				SYNTAX --- STX_FNDEF
				SYNTAX --- STX_IF
				SYNTAX --- STX_WHILE
				SYNTAX --- STX_FOR
				SYNTAX --- STX_DECLARE
				SYNTAX --- STX_ASSIGN
				SYNTAX --- STX_INIT
				SYNTAX --- STX_ECHO
				SYNTAX --- STX_BREAK
				SYNTAX --- STX_RETURN
				SYNTAX --- STX_EXIT
		TOKEN --- TK_RBRACE

----
----

RULE: STX_CLASS
-
	AND
		TOKEN --- TK_CLASS
		TOKEN --- TK_IDENTIFIER
		SYNTAX --- STX_SCOPE

----
----

RULE: STX_FNDEF
-
	AND
		TOKEN --- TK_FN
		TOKEN --- TK_IDENTIFIER
		TOKEN --- TK_LPAREN
		OR
			TOKEN --- TK_BOOL
			TOKEN --- TK_FLT
			TOKEN --- TK_INT
			TOKEN --- TK_STRING
			TOKEN --- TK_IDENTIFIER
		TOKEN --- TK_IDENTIFIER
		IFMANY
			AND
				TOKEN --- TK_COMMA
				OR
					TOKEN --- TK_BOOL
					TOKEN --- TK_FLT
					TOKEN --- TK_INT
					TOKEN --- TK_STRING
					TOKEN --- TK_IDENTIFIER
				TOKEN --- TK_IDENTIFIER
		TOKEN --- TK_RPAREN
		SYNTAX --- STX_SCOPE

----
----

RULE: STX_IF
