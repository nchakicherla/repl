			SYNTAX --- STX_MEMBER
		SYNTAX --- STX_EQUAL
		OR
			SYNTAX --- STX_EXPR
			SYNTAX --- STX_VAR
			SYNTAX --- STX_FNCALL
			SYNTAX --- STX_INDEX
			SYNTAX --- STX_MEMBER
		TOKEN --- TK_SEMICOLON
		OR
			SYNTAX --- STX_GEXPR
			SYNTAX --- STX_EXPR
		TOKEN --- TK_SEMICOLON
		OR
			SYNTAX --- STX_INCREMENT
			SYNTAX --- STX_DECREMENT
		TOKEN --- TK_RPAREN
		IFONE
			OR
				TOKEN --- TK_SEMICOLON
